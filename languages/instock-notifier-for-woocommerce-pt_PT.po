#. Frontend — Form
#: src/Frontend/FormRenderer.php:103
msgid "Want to know when it's back? Leave your email below."
msgstr "Quer saber quando voltar a estar disponível? Deixe o seu email abaixo."

#: src/Frontend/FormRenderer.php:115
msgid "Desired quantity"
msgstr "Quantidade desejada"

#: src/Frontend/FormRenderer.php:134
msgid "Email address"
msgstr "Endereço de email"

#: src/Frontend/FormRenderer.php:135
msgid "Your email address"
msgstr "O seu endereço de email"

#. Frontend — AJAX / error messages
#: src/Frontend/AjaxHandler.php:43
msgid "Security check failed. Please refresh the page."
msgstr "Verificação de segurança falhou. Por favor, atualize a página."

#: src/Frontend/AjaxHandler.php:90
msgid "Something went wrong. Please try again."
msgstr "Algo correu mal. Por favor, tente novamente."

#: src/Frontend/Shortcode.php:108
#: src/Plugin.php:160
msgid "An error occurred."
msgstr "Ocorreu um erro."

#: src/Frontend/Shortcode.php:109
#: src/Plugin.php:161
msgid "An error occurred. Please try again."
msgstr "Ocorreu um erro. Por favor, tente novamente."

#. Frontend — Validation
#: src/Subscription/Validator.php:33
msgid "Spam detected."
msgstr "Spam detetado."

#: src/Subscription/Validator.php:39
msgid "Please enter a valid email address."
msgstr "Por favor, introduza um endereço de email válido."

#: src/Subscription/Validator.php:45
msgid "Invalid product."
msgstr "Produto inválido."

#: src/Subscription/Validator.php:51
msgid "Please accept the consent checkbox."
msgstr "Por favor, aceite a caixa de consentimento."

#: src/Subscription/Validator.php:58
msgid "Too many requests. Please try again later."
msgstr "Demasiados pedidos. Por favor, tente mais tarde."

#. Frontend — Unsubscribe
#: src/Unsubscribe/Handler.php:48
#: src/Unsubscribe/Handler.php:57
msgid "Invalid unsubscribe link."
msgstr "Link de cancelamento inválido."

#: src/Unsubscribe/Handler.php:49
#: src/Unsubscribe/Handler.php:58
#: src/Unsubscribe/Handler.php:66
#: src/Unsubscribe/Handler.php:76
#: src/Unsubscribe/Handler.php:110
msgid "Unsubscribe"
msgstr "Cancelar subscrição"

#: src/Unsubscribe/Handler.php:65
msgid "You have already been unsubscribed."
msgstr "A sua subscrição já foi cancelada."

#: src/Unsubscribe/Handler.php:75
msgid "Security check failed. Please try the link again."
msgstr "Verificação de segurança falhou. Por favor, tente o link novamente."

#: src/Unsubscribe/Handler.php:85
msgid "You have been successfully unsubscribed. You will no longer receive back-in-stock notifications for this product."
msgstr "A sua subscrição foi cancelada com sucesso. Deixará de receber notificações de disponibilidade para este produto."

#: src/Unsubscribe/Handler.php:86
msgid "Unsubscribed"
msgstr "Subscrição cancelada"

#: src/Unsubscribe/Handler.php:100
msgid "Click the button below to confirm you want to unsubscribe from back-in-stock notifications for this product."
msgstr "Clique no botão abaixo para confirmar que pretende cancelar as notificações de disponibilidade para este produto."

#: src/Unsubscribe/Handler.php:104
msgid "Confirm Unsubscribe"
msgstr "Confirmar cancelamento"

#. Frontend — Email content
#: src/Email/BackInStockEmail.php:43
msgid "Back In Stock"
msgstr "De Volta ao Stock"

#: src/Email/BackInStockEmail.php:44
msgid "Sent to customers who subscribed for a back-in-stock notification when the product returns to stock."
msgstr "Enviado a clientes que subscreveram uma notificação de disponibilidade quando o produto regressa ao stock."

#: src/Email/BackInStockEmail.php:56
#: src/Email/BackInStockEmail.php:57
#: src/Email/BackInStockEmail.php:68
#: src/Email/BackInStockEmail.php:77
